generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Leaderboard {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  leaderboard Json
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
